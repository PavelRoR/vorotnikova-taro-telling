#last
	padding 0 0 120px
	background-color: #F4EBE3
	@media screen and (max-width 991px)
		padding 30px 0 90px
		text-align center
	@media screen and (max-width 386px)
		padding 0 0 60px
.last-title
	font-family: L
	font-size: 60px
	color #B18663
	@media screen and (max-width 1199px)
		line-height: 1.3
	@media screen and (max-width 991px)
		font-size: 60px
	@media screen and (max-width 560px)
		font-size: 44px
.last-text
	font-size: 22px
	margin 40px 0 50px
	@media screen and (max-width 560px)
		margin 30px 0 40px
.last-img
	position relative
	// padding-top: 100%
	@media screen and (max-width 991px)
		padding 0
	img
		bottom -290px
		right 0