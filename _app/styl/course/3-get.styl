#get
	padding 90px 0
	@media screen and (max-width: 560px)
		padding 60px 0
	@media screen and (max-width: 386px)
		padding 40px 0
.button-get
	margin-top: 70px
	@media screen and (max-width: 560px)
		margin-top: 40px
.get-img
	position relative
	top 100px
	left 100px
	@media screen and (max-width: 991px)
		position absolute
		left auto
		top auto
		right -100px
		bottom -180px
		width: 200px
		z-index -1
.taro-pic-img
	display block
	width 100%
	margin 0 auto