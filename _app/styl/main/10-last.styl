#last
	padding 100px 0 140px
	@media screen and (max-width: 991px)
		padding 50px 0 90px
		background-color: #F4EBE3
	@media screen and (max-width: 560px)
		margin-top: 20px
		padding 60px 0
.general-img-last
	left -130px
	top calc(50% - 201px)
	width 360px
	z-index -1
	@media screen and (max-width: 991px)
		display block
		position: static
		width 100%
		max-width 482px
		margin 40px auto 0
.form-last
	max-width 750px
	border-radius 35px
	background-color: #FFFBF6
	padding 130px
	margin 0 auto
	@media screen and (max-width: 991px)
		padding 90px 30px
	@media screen and (max-width: 560px)
		padding 60px 30px
	@media screen and (max-width: 386px)
		padding 30px 20px
.input-mail-last
	max-width 467px
	@media screen and (max-width: 991px)
		display block
		margin: 0 auto 10px
.button-form-last
	@media screen and (max-width: 991px)
		display block
		margin 0 auto
		width 100%
		max-width 467px
.checking-last
	@media screen and (max-width: 991px)
		max-width 467px
	@media screen and (max-width: 560px)
		max-width 390px