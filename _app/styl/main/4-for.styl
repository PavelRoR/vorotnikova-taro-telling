#for
	padding 0 0 90px
	@media screen and (max-width: 560px)
		padding 0 0 60px
.button-for
	margin-top: 70px
	@media screen and (max-width: 560px)
		margin-top: 40px
.for-img
	position relative
	top 100px
	left 100px
	width 300px
	@media screen and (max-width: 991px)
		position absolute
		left auto
		top auto
		right -100px
		bottom -180px
		width: 200px
		z-index -1