*
	margin 0
	padding 0
	outline none
body
	font-size: 18px
	font-family: G
	color #000
	background-color #FFFBF6
	line-height: 1.56
.wrapper
	overflow hidden
.content
	position relative
	z-index 30
a
	color: #000
	&:focus
		outline: 0
		text-decoration none
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	top 0
	// z-index 60
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 0
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
		margin 0 auto
.button
	display inline-block
	text-align center
	margin 0 auto
	text-decoration none
	color: #fff;
	width: auto
	font-size: 24px
	font-weight: bold
	padding 14px 65px
	background-color: #3F8FD2
	border-radius: 38px
	transition: all .3s ease
	&:focus
		text-decoration none
		color #fff
	&:hover
		text-decoration none
		color #fff
		background-color: #3F8FD2 - 20%
.general-title
	font-family: L
	font-size: 40px
	margin-bottom: 50px
	color: #B18663
	&-center
		text-align center
.general-img
	position absolute
.general-list
	li
		list-style: none
		position relative
		padding-left: 65px
		margin-bottom: 30px
		font-size: 20px
		&:last-child
			margin-bottom: 0
		span
			font-weight bold
		&:before
			content: ''
			display block
			position absolute
			border-radius: 50px
			width 24px
			height: 24px
			background-color: #B18663
			top 3px
			left 0
	&-2
		li
			list-style: none
			position relative
			padding-left: 25px
			font-size: 20px
			&:before
				content ''
				display block
				position absolute
				width 10px
				height: 10px
				border-radius: 50px
				top 10px
				left 0
				background-color: #D4C1B3